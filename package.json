{
  "name": "component-styleguide-example",
  "version": "1.0.0",
  "description": "Example for component-styleguide",
  "repository": {
    "type": "git",
    "url": "http://github.com/webpro/component-styleguide-example.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node index.js",
    "build": "browserify js/index.js -vo compiled/bundle.js && node-sass scss/stylesheet.scss -o compiled",
    "dev": "nodemon -w components -w data -e js,json,html index.js & watchify js/index.js -vo compiled/bundle.js & node-sass -rw scss scss/stylesheet.scss -o compiled"
  },
  "dependencies": {
    "browserify": "^9.0.3",
    "component-styleguide": "^1.1.0",
    "node-sass": "^2.0.1"
  },
  "devDependencies": {
    "nodemon": "^1.3.7",
    "watchify": "^2.4.0"
  },
  "author": {
    "name": "Lars Kappert",
    "email": "lars@webpro.nl",
    "url": "http://webpro.nl"
  },
  "license": "MIT"
}
